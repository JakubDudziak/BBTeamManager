---
import PlayersManager from "./react/PlayersManager";

const res = await fetch(new URL("/api/players", Astro.url));
const data = await res.json();
---

<div class="flex justify-center">
    <div
        class="flex flex-col w-full m-6 bg-white border-solid border-2 border-(--middle-gray) rounded-2xl"
    >
        <PlayersManager client:load initialItems={data} />
    </div>
</div>
